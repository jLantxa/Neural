<?xml version = "1.0"?>
<NeuralNetwork>
    <Layer name="layer0" behaviour="identity">
        <Neuron>0.0</Neuron>
        <Neuron>0.1</Neuron>
        <Neuron>0.2</Neuron>
    </Layer>

    <Layer name="layer1" behaviour="logistic">
        <Neuron>1.0</Neuron>
        <Neuron>1.1</Neuron>
    </Layer>

    <Connection layer="layer0">
        <Node>
            <Weight>0.0</Weight>
            <Weight>0.1</Weight>
        </Node>

        <Node>
            <Weight>1.0</Weight>
            <Weight>1.1</Weight>
        </Node>

        <Node>
            <Weight>2.0</Weight>
            <Weight>2.1</Weight>
        </Node>
    </Connection>
</NeuralNetwork>
